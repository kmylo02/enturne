<IfModule mod_rewrite.c>
    RewriteEngine On     
    RewriteCond %{REQUEST_FILENAME} !-f 
    RewriteCond %{REQUEST_FILENAME} !-d 
    RewriteCond $1 !^(index\.php|images|robots\.txt)
    RewriteRule ^(.*)$ index.php/$1 [L] 
</IfModule>
<IfModule !mod_rewrite.c>
    ErrorDocument 404 
</IfModule>
<Files 403.shtml>
order allow,deny
allow from all
</Files>
